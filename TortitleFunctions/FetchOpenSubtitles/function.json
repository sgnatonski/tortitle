{
  "bindings": [
    {
      "name": "timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */10 * * * *",
      "runOnStartup": true
    },
    {
      "name": "torrentsMarksTable",
      "type": "table",
      "tableName": "torrentmarks",
      "connection": "AzureWebJobsStorage",
      "direction": "inout"
    },
    {
      "name": "subtitlesTable",
      "type": "table",
      "tableName": "subtitles",
      "connection": "AzureWebJobsStorage",
      "direction": "inout"
    },
    {
      "name": "languagesTable",
      "type": "table",
      "tableName": "languages",
      "connection": "AzureWebJobsStorage",
      "direction": "inout"
    }
  ],
  "disabled": false
}